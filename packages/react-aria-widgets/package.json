{
  "name": "react-aria-widgets",
  "version": "0.1.3",
  "description": "A collection of React components designed to help developers implement the patterns found in the ARIA Authoring Practices Guide (APG).",
  "keywords": [
    "react",
    "aria",
    "accessible",
    "a11y",
    "accessibility"
  ],
  "author": "Charlie Yao <charlie-yao@users.noreply.github.com>",
  "homepage": "https://charlieyao.com/react-aria-widgets",
  "license": "MIT",
  "main": "dist/index.bundle.js",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": "./dist/index.bundle.js",
    "./accordion": "./dist/accordion.bundle.js",
    "./styles.css": "./dist/index.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlie-yao/react-aria-widgets.git"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "lint": "eslint --fix --ext .jsx,.js ./",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish": "yarn npm publish"
  },
  "bugs": {
    "url": "https://github.com/charlie-yao/react-aria-widgets/issues"
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "peerDependencies": {
    "react": "^17"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@mdx-js/react": "^1.6",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.36.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "mini-css-extract-plugin": "^2.7.3",
    "react": "^18.2",
    "react-dom": "^18.2",
    "sass": "1.59.2",
    "sass-loader": "^13.2.0",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  }
}
